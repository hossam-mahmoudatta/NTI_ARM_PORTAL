#ifndef STD_TYPES_H
#define STD_TYPES_H

/*******************************************************************************
 *                              						Macros Declarations	                         					  *
 *******************************************************************************/

typedef unsigned char        u8 ;
typedef unsigned short int   u16;
typedef unsigned long  int   u32; 


